.hover1:hover {
    background-color: rgba(236, 236, 236, 255);
    rotate: 90deg;
    transition-duration: 0.5s;
    transition-delay: 0s;
    transition-timing-function: ease-out-back;
    border-left-width: 0;
    border-right-width: 0;
    border-top-width: 0;
    border-bottom-width: 0;
    min-height: 1px;
    min-width: 1px;
    scale: 1.3 1.3;
    translate: 0 0;
    transform-origin: center;
}
